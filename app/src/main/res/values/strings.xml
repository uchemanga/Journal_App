<?xml version="1.0" encoding="utf-8"?>

<resources xmlns:xliff="http://schemas.android.com/apk/res-auto">
    <!--Used in Action Bar, and in AndroidManifest to tell the device the name of this app.-->
    <!--It's good to keep this short, so your launcher icon isn't displayed with "The greatest Wea"-->
    <!--or something similar.-->
    <string name="app_name">JOURNAL APP</string>

    <!--Used in overflow menu to diplay journal data-->
    <string name="list_journal">JournalList</string>

    <!-- Label for overflow menu option that inserts fake JounalApp data into the app [CHAR LIMIT=20] -->
    <string name="action_insert_dummy_data">Insert Dummy Data</string>

    <!-- Label for overflow menu option that deletes all JounalApp data in the app [CHAR LIMIT=20] -->
    <string name="action_delete_all_entries">Delete All JounalApp</string>

    <!-- Title text for the empty view, which describes the empty dog house image [CHAR LIMIT=50] -->
    <string name="empty_view_title_text">It\'s a bit lonely here...</string>

    <!-- Subtitle text for the empty view that prompts the user to add a JounalApp [CHAR LIMIT=50] -->
    <string name="empty_view_subtitle_text">Get started by adding a JounalApp</string>

    <!-- Title for the activity to add a new JounalApp [CHAR LIMIT=20] -->
    <string name="editor_activity_title_new_JounalApp">Add a JounalApp</string>

    <!-- Title for the activity to edit an existing JounalApp [CHAR LIMIT=20] -->
    <string name="editor_activity_title_edit_JounalApp">Edit JounalApp</string>

    <!-- Label for editor menu option to save JounalApp and leave editor [CHAR LIMIT=20] -->
    <string name="action_save">Save</string>

    <!-- Label for editor overflow menu option that deletes the current JounalApp [CHAR LIMIT=20] -->
    <string name="action_delete">Delete</string>

    <!-- Toast message in editor when new JounalApp has been successfully inserted [CHAR LIMIT=NONE] -->
    <string name="editor_insert_JounalApp_successful">JounalApp saved</string>

    <!-- Toast message in editor when new JounalApp has failed to be inserted [CHAR LIMIT=NONE] -->
    <string name="editor_insert_JounalApp_failed">Error with saving JounalApp</string>

    <!-- Toast message in editor when current JounalApp was successfully updated [CHAR LIMIT=NONE] -->
    <string name="editor_update_JounalApp_successful">JounalApp updated</string>

    <!-- Toast message in editor when current JounalApp has failed to be updated [CHAR LIMIT=NONE] -->
    <string name="editor_update_JounalApp_failed">Error with updating JounalApp</string>

    <!-- Dialog message when user is leaving editor but hasn't saved changes [CHAR LIMIT=NONE] -->
    <string name="unsaved_changes_dialog_msg">Discard your changes and quit editing?</string>

    <!-- Dialog button text for the option to discard a user's changes [CHAR LIMIT=20] -->
    <string name="discard">Discard</string>

    <!-- Dialog button text for the option to keep editing the current JounalApp [CHAR LIMIT=20] -->
    <string name="keep_editing">Keep Editing</string>

    <!-- Toast message in editor when current JounalApp was successfully deleted [CHAR LIMIT=NONE] -->
    <string name="editor_delete_JounalApp_successful">JounalApp deleted</string>

    <!-- Toast message in editor when current JounalApp has failed to be deleted [CHAR LIMIT=NONE] -->
    <string name="editor_delete_JounalApp_failed">Error with deleting JounalApp</string>

    <!-- Dialog message to ask the user to confirm deleting the current JounalApp [CHAR LIMIT=NONE] -->
    <string name="delete_dialog_msg">Delete this JounalApp?</string>

    <!-- Dialog button text for the option to confirm deleting the current JounalApp [CHAR LIMIT=20] -->
    <string name="delete">Delete</string>

    <!-- Dialog button text for the option to cancel deletion of the current JounalApp [CHAR LIMIT=20] -->
    <string name="cancel">Cancel</string>

    <!-- Label for overview category of attributes in the editor [CHAR LIMIT=30] -->
    <string name="category_overview">Overview</string>

    <!-- Label for gender information in the editor [CHAR LIMIT=30] -->
    <string name="category_gender">Gender</string>

    <!-- Label for measurement information in the editor [CHAR LIMIT=30] -->
    <string name="category_measurement">Measurement</string>

    <!-- Text hint for name field in the editor [CHAR LIMIT=30] -->
    <string name="hint_JounalApp_name">Name</string>

    <!-- Text hint for breed field in the editor [CHAR LIMIT=30] -->
    <string name="hint_JounalApp_breed">Breed</string>

    <!-- Text hint for name field in the editor [CHAR LIMIT=30] -->
    <string name="hint_JounalApp_weight">Weight</string>

    <!-- Units for weight of the JounalApp (kilograms) [CHAR LIMIT=5] -->
    <string name="unit_JounalApp_weight">kg</string>

    <!-- Label for dropdown menu option if the JounalApp's gender is unknown [CHAR LIMIT=20] -->
    <string name="gender_unknown">Unknown</string>

    <!-- Label for dropdown menu option if the JounalApp is male [CHAR LIMIT=20] -->
    <string name="gender_male">Male</string>

    <!-- Label for dropdown menu option if the JounalApp is female [CHAR LIMIT=20] -->
    <string name="gender_female">Female</string>

    <!-- Label for the JounalApp's breed if the breed is unknown [CHAR LIMIT=20] -->
    <string name="empty_journal">Empty Journal</string>


</resources>
